{
    "name": "webeweb/jquery-querybuilder-bundle",
    "type": "symfony-bundle",
    "description": "Integrate jQuery QueryBuilder with Symfony 2 and more",
    "keywords": [
        "bundle",
        "jquery",
        "querybuilder",
        "symfony",
        "webeweb"
    ],
    "homepage": "https://github.com/webeweb/jquery-querybuilder-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "symfony/framework-bundle": "^3.4|^4.0",
        "twig/twig": "^1.0||^2.0",
        "webeweb/core-bundle": "^2.20",
        "webeweb/core-library": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^9.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.7",
        "sensio/framework-extra-bundle": "^3.4|^4.0",
        "monolog/monolog": "^1.18",
        "symfony/browser-kit": "^3.4|^4.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/security-bundle": "^3.4|^4.0",
        "symfony/templating": "^3.4|^4.0",
        "symfony/twig-bundle": "^3.4|^4.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WBW\\Bundle\\JQuery\\QueryBuilderBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Bundle\\JQuery\\QueryBuilderBundle\\Tests\\": "Tests/"
        }
    },
    "support": {
        "issues": "https://github.com/webeweb/jquery-querybuilder-bundle/issues"
    }
}
